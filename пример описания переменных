int a;           //1 глобальная перерменная a

int maain()
{
  int b;        //2 локальная переменная b
  extern int x; //3 локальная переменная х определена в другом месте
  static c;      //4 локальная статическая переменная с
  a=1;           //5 присваивание глобальной переменной
  int a;        //6 локальная переменная ф
  a=2;          //7 присвоение локальной переменной
  ::a=3;        //8 присловение глобальной переменной
  return 0;
}
int x=4;        //9 определение и инициализация х
